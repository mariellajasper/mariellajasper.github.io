{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Real GDP in Great Britain, 1955-2024",
    "anchor": "start",
    "subtitle": "Real GDP, quarterly, Â£ million | Source: ONS through ECO API",
