{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "International Territorial Level Map, to change regional hierarchy: replace x in 'ITLx_2021_20m' in URL, and 'ITLx21NM' in tooltip, with 1, 2 or 3.",
  "title": {
    "text": "Germany",
    "subtitle": ["Federal States, Mercator Projection", ""],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "middle",
    "color": "black"
  },
  "config": {"background": "white", "view": {"stroke": "transparent"}},
  "width": 300,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/mariellajasper/mariellajasper.github.io/refs/heads/main/germany_map.geo.json",
    "format": {"property": "features"}
  },
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "fill": "rgb(50,50,50)",
    "stroke": "white",
    "strokeWidth": 0.5
  },
  "encoding": {"tooltip": {"field": "properties.name"}}
}